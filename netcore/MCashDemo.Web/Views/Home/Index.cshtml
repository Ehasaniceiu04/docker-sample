                                                <tr>
                                                    <td>@Model.TransactionDetails[i].Id</td>
                                                    <td>@Model.TransactionDetails[i].TransactionDate.ToString("dd MMM yyyy")</td>
                                                    <td>@Model.TransactionDetails[i].TransactionDate.ToString("dd MMM yyyy")</td>
                                                    <td>@Model.TransactionDetails[i].Reference</td>
                                                    <td>@Model.TransactionDetails[i].Description</td>
                                                    <td>@Model.TransactionDetails[i].Debit</td>
                                                    <td>@Model.TransactionDetails[i].Credit</td>
                                                    <td>@Model.TransactionDetails[i].Balance</td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>

