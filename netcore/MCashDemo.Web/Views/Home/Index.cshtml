                                            <th>Reference </th>
                                            <th>Description </th>
                                            <th>Debit </th>
                                            <th>Credit</th>
                                            <th>Balance</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.TransactionDetails != null && Model.TransactionDetails.Count > 0)
                                        {
                                            @for (var i = 0; i < Model.TransactionDetails.Count(); ++i)
